{
  "expo": {
    "name": "Foodever21",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./App.js",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company.foodever21"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.company.foodever21",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "debuggerHost": "192.168.1.254:8081",
      "enableHermes": true
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to set your profile picture."
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "scheme": "frontend",
    "extra": {
      "projectId": "foodever21-7618e",
      "eas": {
        "projectId": "42dc1502-f3fd-4acc-8c5f-7deb258d4ad5"
      }
    },
    "owner": "theneelothevis" 
  }
}
